{
  "source": {
    "include": [
      "./controllers",
      "./database",
      "./middlewares",
      "./models",
      "./routes",
      "./index.js"
    ],
    "exclude": []
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs",
    "recurse": true
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
